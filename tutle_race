#!/bin/python3
from turtle import *
from random import randint

for i in range(10):
  speed(0)
  write(i, align='left')
  right(90)
  forward(10)
  pendown()
  forward(150)
  penup()
  backward(160)
  left(90)
  forward(20)
ada =Turtle()
ada.penup()
ada.color('#0ff')
ada.goto(0 ,-130)
ada.shape('turtle')
aa =Turtle()
aa.penup()
aa.color('#00f')
aa.goto(0 ,-160)
aa.shape('turtle')
ad =Turtle()
ad.penup()
ad.color('#f60')
ad.goto(0 ,-100)
ad.shape('turtle')
ba =Turtle()
ba.penup()
ba.color('#055')
ba.goto(0 ,-40)
ba.shape('turtle')
a =Turtle()
a.penup()
a.color('#f06')
a.goto(0 ,-70)
a.shape('turtle')
for turn in range(90):
  ada.forward(randint(1,5))
  aa.forward(randint(1,5))
  ba.forward(randint(1,5))
  ad.forward(randint(1,5))
  a.forward(randint(1,5))
  
